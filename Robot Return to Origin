class Solution {
  bool judgeCircle(String moves) {
    int x = 0;
    int y = 0;

    for (var move in moves.split('')) {
      if (move == 'U') y++;
      else if (move == 'D') y--;
      else if (move == 'R') x++;
      else if (move == 'L') x--;
    }

    return x == 0 && y == 0;
  }
}

void main() {
  Solution sn = Solution();
  print(sn.judgeCircle("UD")); 
  print(sn.judgeCircle("LL"));  
  print(sn.judgeCircle("UDR"));  
}
