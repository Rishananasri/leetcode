class Solution {
  bool threeConsecutiveOdds(List<int> arr) {
    int count = 0;

    for (int num in arr) {
      if (num % 2 != 0) {
        count++;
        if (count == 3) {
          return true;
        }
      } else {
        count = 0;
      }
    }

    return false;
  }
}

void main() {
  Solution sn = Solution();
  print(sn.threeConsecutiveOdds([2, 6, 4, 1]));
  print(sn.threeConsecutiveOdds([1, 2, 34, 3, 4, 5, 7, 23, 12]));
}
