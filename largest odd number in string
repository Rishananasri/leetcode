String largestOddNumber(String num) {
  for (int i = num.length - 1; i >= 0; i--) {
    int digit = int.parse(num[i]);
    if (digit % 2 != 0) {
      return num.substring(0, i + 1);
    }
  }
  return "";
}
void main() {
  print(largestOddNumber("52"));     
  print(largestOddNumber("4206"));   
  print(largestOddNumber("35427"));   
}

